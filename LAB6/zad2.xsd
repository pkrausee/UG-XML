<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="telefon">
        <xsd:simpleType>
            <xsd:restriction base="xsd:token">
                <xsd:pattern value="[0-9]{3}-[0-9]{3}-[0-9]{3}|\+48 [0-9]{3} [0-9]{3} [0-9]{3}|\(58\) [0-9]{3} [0-9]{2} [0-9]{2}"/>
            </xsd:restriction> 
        </xsd:simpleType>
    </xsd:element>
</xsd:schema>